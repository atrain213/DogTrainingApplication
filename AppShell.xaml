<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    xmlns:pages="clr-namespace:MauiApp1.Pages" >

    <TabBar>
        <Tab
            Title = "Home">
            <ShellContent ContentTemplate="{DataTemplate local:HomePage}" />
            
        </Tab>

        <Tab
            Title = "Dogs">
            <ShellContent ContentTemplate="{DataTemplate local:DogsPage}" />

        </Tab>

        <Tab
            Title = "Account">
            <ShellContent ContentTemplate="{DataTemplate local:AccountPage}" />

        </Tab>

        <Tab
            Title = "Main Page">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />

        </Tab>
        <Tab
            Title = "Test Page">
            <ShellContent ContentTemplate="{DataTemplate local:TestPage}" />

        </Tab>
    </TabBar>

</Shell>
